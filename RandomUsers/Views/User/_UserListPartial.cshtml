@model List<RandomUsers.Models.UserData>

<table class="table">

    <tbody>
    @for (var i = 0; i < Model.Count; i++)
    {
        var user = Model[i];
        <tr>
            <td>@(i + 1)</td>
            <td>@user.Id</td>
            <td>@user.FullName</td>
            <td>@user.Address</td>
            <td>@user.PhoneNumber</td>
        </tr>
    }
    </tbody>
</table>